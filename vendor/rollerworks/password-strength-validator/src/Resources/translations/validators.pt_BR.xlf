<?xml version="1.0"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file source-language="en" datatype="plaintext" original="file.ext">
        <body>
            <trans-unit id="1">
                <source>Your password must be at least {{length}} characters long.</source>
                <target>A senha deve ter pelo menos {{length}} caracteres de comprimento.</target>
            </trans-unit>
            <trans-unit id="2">
                <source>Your password must include at least one letter.</source>
                <target>A senha deve conter pelo menos uma letra.</target>
            </trans-unit>
            <trans-unit id="3">
                <source>Your password must include both upper and lower case letters.</source>
                <target>A senha deve conter letras maiúsculas e minúsculas.</target>
            </trans-unit>
            <trans-unit id="4">
                <source>Your password must include at least one number.</source>
                <target>A senha deve conter pelo menos um número.</target>
            </trans-unit>
            <trans-unit id="5">
                <source>Your password must contain at least one special character.</source>
                <target>A senha deve conter pelo menos um caractere especial.</target>
            </trans-unit>
            <trans-unit id="6">
                <source>password_too_weak</source>
                <target>A senha deve ter pelo menos o nível de intensidade "{{ min_strength }}" e o nível atual é "{{ current_strength }}". Tente o seguinte: {{ strength_tips }}.</target>
            </trans-unit>

            <!-- Strength levels -->
            <trans-unit id="7">
                <source>rollerworks_password.strength_level.very_weak</source>
                <target>Muito fraco</target>
            </trans-unit>
            <trans-unit id="8">
                <source>rollerworks_password.strength_level.weak</source>
                <target>Fraco</target>
            </trans-unit>
            <trans-unit id="9">
                <source>rollerworks_password.strength_level.medium</source>
                <target>Médio</target>
            </trans-unit>
            <trans-unit id="10">
                <source>rollerworks_password.strength_level.strong</source>
                <target>Forte</target>
            </trans-unit>
            <trans-unit id="11">
                <source>rollerworks_password.strength_level.very_strong</source>
                <target>Muito forte</target>
            </trans-unit>

            <!-- Tip translations -->
            <trans-unit id="12">
                <source>rollerworks_password.tip.letters</source>
                <target>adicionar letras (maiúsculas/minúsculas)</target>
            </trans-unit>
            <trans-unit id="13">
                <source>rollerworks_password.tip.numbers</source>
                <target>adicionar números</target>
            </trans-unit>
            <trans-unit id="14">
                <source>rollerworks_password.tip.lowercase_letters</source>
                <target>adicionar letras minúsculas</target>
            </trans-unit>
            <trans-unit id="15">
                <source>rollerworks_password.tip.uppercase_letters</source>
                <target>adicionar letras maiúsculas</target>
            </trans-unit>
            <trans-unit id="16">
                <source>rollerworks_password.tip.special_chars</source>
                <target>adicionar caracteres especiais</target>
            </trans-unit>
            <trans-unit id="17">
                <source>rollerworks_password.tip.length</source>
                <target>adicionar mais caracteres</target>
            </trans-unit>

            <!-- P0wnedPassword -->
            <trans-unit id="18">
                <source>This password was found in a database of compromised passwords. It has been used {{ used }} times. For security purposes you must use something else.</source>
                <target>Esta senha foi encontrada em um banco de dados de senhas comprometidas. Foi usada {{used}} vezes. Por motivos de segurança, você deve usar outra senha.</target>
            </trans-unit>
        </body>
    </file>
</xliff>

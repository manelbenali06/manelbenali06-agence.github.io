<?xml version="1.0"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file source-language="en" datatype="plaintext" original="file.ext">
        <body>
            <trans-unit id="1">
                <source>Your password must be at least {{length}} characters long.</source>
                <target>Heslo musí mít alespoň {{length}} znaků.</target>
            </trans-unit>
            <trans-unit id="2">
                <source>Your password must include at least one letter.</source>
                <target>Heslo musí obsahovat písmeno.</target>
            </trans-unit>
            <trans-unit id="3">
                <source>Your password must include both upper and lower case letters.</source>
                <target>Heslo musí obsahovat velká i malá písmena.</target>
            </trans-unit>
            <trans-unit id="4">
                <source>Your password must include at least one number.</source>
                <target>Heslo musí obsahovat číslici.</target>
            </trans-unit>
            <trans-unit id="5">
                <source>Your password must contain at least one special character.</source>
                <target>Heslo musí obsahovat speciální znak.</target>
            </trans-unit>
            <trans-unit id="6">
                <source>password_too_weak</source>
                <target>Úroveň hesla musí být alespoň "{{ min_strength }}", současná úroveň je "{{ current_strength }}", zkuste {{ strength_tips }}.</target>
            </trans-unit>

            <!-- Strength levels -->
            <trans-unit id="7">
                <source>rollerworks_password.strength_level.very_weak</source>
                <target>velmi slabé</target>
            </trans-unit>
            <trans-unit id="8">
                <source>rollerworks_password.strength_level.weak</source>
                <target>slabé</target>
            </trans-unit>
            <trans-unit id="9">
                <source>rollerworks_password.strength_level.medium</source>
                <target>střední</target>
            </trans-unit>
            <trans-unit id="10">
                <source>rollerworks_password.strength_level.strong</source>
                <target>silné</target>
            </trans-unit>
            <trans-unit id="11">
                <source>rollerworks_password.strength_level.very_strong</source>
                <target>velmi silné</target>
            </trans-unit>

            <!-- Tip translations -->
            <trans-unit id="12">
                <source>rollerworks_password.tip.letters</source>
                <target>přidat malá/velká písmena</target>
            </trans-unit>
            <trans-unit id="13">
                <source>rollerworks_password.tip.numbers</source>
                <target>přidat číslici</target>
            </trans-unit>
            <trans-unit id="14">
                <source>rollerworks_password.tip.lowercase_letters</source>
                <target>přidat malé písmeno</target>
            </trans-unit>
            <trans-unit id="15">
                <source>rollerworks_password.tip.uppercase_letters</source>
                <target>přidat velké písmeno</target>
            </trans-unit>
            <trans-unit id="16">
                <source>rollerworks_password.tip.special_chars</source>
                <target>přidat speciální znak</target>
            </trans-unit>
            <trans-unit id="17">
                <source>rollerworks_password.tip.length</source>
                <target>delší heslo</target>
            </trans-unit>

            <!-- P0wnedPassword -->
            <trans-unit id="18">
                <source>This password was found in a database of compromised passwords. It has been used {{ used }} times. For security purposes you must use something else.</source>
                <target>Toto heslo bylo nalezeno v databázi ohrožených hesel. Byl používán {{used}} časy. Z bezpečnostních důvodů musíte použít něco jiného.    </target>
            </trans-unit>
        </body>
    </file>
</xliff>
